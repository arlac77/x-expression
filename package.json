{
  "name": "x-expression",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/grammar.mjs",
  "module": "src/grammar.mjs",
  "description": "simple expressions for x-path like operations",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/grammar.mjs --section=API",
    "lint": "documentation lint src/grammar.mjs",
    "test": "ava"
  },
  "dependencies": {
    "pratt-parser": "^8.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "semantic-release": "^17.0.2"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/x-expression.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/x-expression/issues"
  },
  "homepage": "https://github.com/arlac77/x-expression#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
